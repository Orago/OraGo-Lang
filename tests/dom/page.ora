LET body = qs('body');
body.styles({ background: 'red' });
body.append('YOO THIS IS FIRE');



FUNCTION run (){
	LET c = [Math.random(), Math.random(), Math.random()] * 255;

	LET background = 'rgb('+ c.0 + ',' + c.1 + ',' + c.2 + ')';


	PRINT background;
	body.styles({
		background
	});
}

interval(run, 1000);