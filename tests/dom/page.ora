func panel (){
	let el = n.div
		.styles({
			padding: '5px',
			borderRadius: '5px',
			margin: '5px',
			background: 'white',
			transition: '5s'
		});

	func color (){
		let c = [Math.random(), Math.random(), Math.random()] * 255;

		let background = 'rgb('+ c.0 + ',' + c.1 + ',' + c.2 + ')';

		el.styles({ background });
	}

	interval(color, 2000);
	
	return el;
}


